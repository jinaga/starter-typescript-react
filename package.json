{
  "name": "myapplication",
  "version": "1.0.0",
  "description": "Brand new Jinaga Application",
  "private": true,
  "main": "dist/server/server.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "webpack && tsc --project src/server && npm run build:copy",
    "build:copy": "copyfiles -f views/login.html dist/server && copyfiles -f src/server/images/* dist/server/images",
    "start": "node -r tsconfig-paths/register dist/server/server",
    "dev": "npm run build:copy && concurrently -k \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "NODE_ENV=development tsc-watch --project src/server --onSuccess \"npm start\"",
    "dev:client": "webpack --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Michael L Perry",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "express-session": "^1.17.2",
    "jimp": "^0.16.1",
    "jinaga": "^3.0.4",
    "jinaga-react": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.5.2",
    "passport-twitter": "^1.0.4",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsconfig-paths": "^3.14.1",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.17",
    "@types/passport": "^1.0.4",
    "@types/passport-twitter": "^1.0.36",
    "@types/pg": "^8.6.3",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "concurrently": "^6.2.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.4.5",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  }
}
